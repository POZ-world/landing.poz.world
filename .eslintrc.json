{
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true
  },
  "plugins": [
    "react",
    "import"
  ],
  "extends": [
  ],
  "noInlineConfig": false,
  "root": true,
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "no-console": "off",
        "no-undef": "off",
        "no-unused-vars":"off",
        "import/no-default-export": "off"
      }
    }
  ],
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-unused-vars":"off",
    "import/no-default-export": "off"
  }
}