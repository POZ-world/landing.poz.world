{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run landing.poz.world",
            "command": "npm",
            "args": [
                "run",
                "start:local"
            ],
            "type": "process",
            "options": {
                "env": {
                    "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
                },
                "cwd": "${workspaceFolder}",
                "shell": {
                    "executable": "bash"
                }
            }
        }
    ]
}